{
  "name": "SB Moters",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && mkdir -p ../backend/public && rm  -rf ../backend/public/* && cp -r dist/* ../backend/public",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "json-server -p3001 --watch db.json",
    "postinstall": "npm run build && mkdir -p ../backend/public && rm  -rf ../backend/public/* && cp -r dist/* ../backend/public"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "axios": "^1.7.2",
    "balloons-js": "^0.0.2",
    "npm": "^10.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-number-format": "^5.4.3",
    "react-router-dom": "^7.2.0",
    "react-to-print": "^2.14.8"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "json-server": "^1.0.0-beta.1",
    "vite": "^6.3.5"
  }
